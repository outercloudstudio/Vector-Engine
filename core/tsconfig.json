{
	"compilerOptions": {
		"baseUrl": "src",
		"outDir": "lib",
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"lib": ["ESNext", "DOM"],
		"declaration": true,
		"sourceMap": true
	}
}
